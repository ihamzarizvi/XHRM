put-field\n    type=\"autocomplete\"\n    :clear=\"false\"\n    :label=\"$t('general.name')\"\n    :create-options=\"loadTypes\"\n  >\n    <template #option=\"{data}\">\n      <span> {{ data.label }} </span>\n    </template>\n  </oxd-input-field>\n</template>\n\n<script>\nimport {APIService} from '@ohrm/core/util/services/api.service';\nexport default {\n  name: 'ExpenseTypeAutocomplete',\n  setup() {\n    const http = new APIService(\n      window.appGlobal.baseUrl,\n      '/api/v2/claim/expenses/types',\n    );\n    return {\n      http,\n    };\n  },\n  methods: {\n    async loadTypes(serachParam) {\n      return new Promise((resolve) => {\n        if (serachParam.trim()) {\n          const params = {\n            name: serachParam.trim(),\n          };\n          this.http.getAll(params).then(({data}) => {\n            resolve(\n              data.data.map((type) => {\n                return {\n                  id: type.id,\n                  label: type.name,\n                };\n              }),\n            );\n          });\n        } else {\n          resolve([]);\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\nspan {\n  word-break: break-word;\n}\n</style>\n\r\n"],"mappings":";;;uBAoBEA,YAAA,CASkBC,0BAAA;IARhBC,IAAI,EAAC,cAAc;IAClBC,KAAK,EAAE,KAAK;IACZC,KAAK,EAAEC,IAAA,CAAAC,EAAE;IACT,gBAAc,EAAEC,QAAA,CAAAC;;IAENC,MAAM,EAAAC,QAAA,CACf,CAA+B;MADbC;IAAI,OACtBC,mBAAA,CAA+B,cAAAC,gBAAA,CAArBF,IAAI,CAACP,KAAK,M","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}