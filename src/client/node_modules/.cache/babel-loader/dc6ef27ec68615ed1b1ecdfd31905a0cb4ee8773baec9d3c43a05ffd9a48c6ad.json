ta.entitlement?.updateAs\n            ? parseFloat(data.entitlement.updateAs).toFixed(2)\n            : '0.00';\n          return new Promise((resolve, reject) => {\n            this.resolve = resolve;\n            this.reject = reject;\n            this.show = true;\n          });\n        });\n    },\n    onConfirm() {\n      this.show = false;\n      this.resolve && this.resolve('ok');\n    },\n    onCancel() {\n      this.show = false;\n      this.resolve && this.resolve('cancel');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.XHRM-modal-header {\n  margin-bottom: 1.2rem;\n  display: flex;\n  justify-content: center;\n}\n.XHRM-modal-footer {\n  margin-top: 1.2rem;\n  display: flex;\n  justify-content: center;\n}\n.XHRM-button-margin {\n  margin: 0.25rem;\n}\n.XHRM-text-center-align {\n  text-align: center;\n}\n</style>\n\r\n"],"mappings":";;EA0BWA,KAAK,EAAC;AAAmB;;EAKzBA,KAAK,EAAC;AAAwB;;EAU9BA,KAAK,EAAC;AAAmB;;;;;uBArBlCC,YAAA,CAoCWC,SAAA;IApCDC,EAAE,EAAC;EAAM,IAETC,KAAA,CAAAC,IAAI,I,cADZJ,YAAA,CAkCaK,qBAAA;;IAhCVC,KAAK,EAAE;MAAAC,QAAA;IAAA,CAAmB;IAC1B,eAAW,EAAEC,QAAA,CAAAC;;sBAEd,MAIM,CAJNC,mBAAA,CAIM,OAJNC,UAIM,GAHJC,YAAA,CAEWC,mBAAA;MAFDC,IAAI,EAAC;IAAY;wBACzB,MAAsC,C,kCAAnCC,IAAA,CAAAC,EAAE,oC;;UAGTN,mBAAA,CASM,OATNO,UASM,GARJL,YAAA,CAOWC,mBAAA;MAPDC,IAAI,EAAC;IAAW;wBACxB,MAKE,C,kCAJAC,IAAA,CAAAC,EAAE;kBAA2Eb,KAAA,CAAAe,OAAO;kBAA0Bf,KAAA,CAAAgB;;;UAOpHT,mBAAA,CAaM,OAbNU,UAaM,GAZJR,YAAA,CAKES,qBAAA;MAJA,cAAY,EAAC,OAAO;MACpBtB,KAAK,EAAC,oBAAoB;MACzBuB,KAAK,EAAEP,IAAA,CAAAC,EAAE;MACTO,OAAK,EAAEf,QAAA,CAAAC;uCAEVG,YAAA,CAKES,qBAAA;MAJA,cAAY,EAAC,WAAW;MACxBtB,KAAK,EAAC,oBAAoB;MACzBuB,KAAK,EAAEP,IAAA,CAAAC,EAAE;MACTO,OAAK,EAAEf,QAAA,CAAAgB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}