div>\n        <div class=\"XHRM-edit-employee-content\">\n          <slot></slot>\n          <profile-custom-fields\n            v-if=\"screen !== 'default'\"\n            :employee-id=\"employeeId\"\n            :screen=\"screen\"\n          ></profile-custom-fields>\n          <profile-attachments\n            v-if=\"screen !== 'default'\"\n            :employee-id=\"employeeId\"\n            :allowed-file-types=\"allowedFileTypes\"\n            :max-file-size=\"maxFileSize\"\n            :screen=\"screen\"\n          ></profile-attachments>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {computed, ref} from 'vue';\nimport {APIService} from '@/core/util/services/api.service';\nimport {navigate} from '@ohrm/core/util/helper/navigation';\nimport TabsNavigation from '@/XHRMPimPlugin/components/TabsNavigation';\nimport ProfileAttachments from '@/XHRMPimPlugin/components/ProfileAttachments';\nimport ProfileCustomFields from '@/XHRMPimPlugin/components/ProfileCustomFields';\n\nconst defaultPic = `${window.appGlobal.publicPath}/images/default-photo.png`;\n\nexport default {\n  name: 'EditEmployeeLayout',\n  components: {\n    'tabs-navigation': TabsNavigation,\n    'profile-attachments': ProfileAttachments,\n    'profile-custom-fields': ProfileCustomFields,\n  },\n  props: {\n    employeeId: {\n      type: String,\n      required: true,\n    },\n    tabs: {\n      type: Array,\n      required: true,\n    },\n    allowedFileTypes: {\n      type: Array,\n      required: true,\n    },\n    maxFileSize: {\n      type: Number,\n      required: true,\n    },\n    screen: {\n      type: String,\n      default: 'default',\n      validator(value) {\n        return [\n          'default',\n          'personal',\n          'contact',\n          'emergency',\n          'dependents',\n          'immigration',\n          'qualifications',\n          'tax',\n          'salary',\n          'job',\n          'report-to',\n          'membership',\n        ].includes(value);\n      },\n    },\n  },\n  setup(props) {\n    const employeeName = ref('');\n    const isCurrentEmp = ref(true);\n    const http = new APIService(\n      window.appGlobal.baseUrl,\n      '/api/v2/pim/employees',\n    );\n\n    http.get(props.employeeId).then(({data}) => {\n      employeeName.value = `${data.data.firstName} ${data.data.lastName}`;\n      isCurrentEmp.value = data.data.terminationId ? false : true;\n    });\n\n    const imgSrc = computed(() => {\n      return props.employeeId\n        ? `${window.appGlobal.baseUrl}/pim/viewPhoto/empNumber/${props.employeeId}`\n        : defaultPic;\n    });\n\n    const onClickProfilePic = () => {\n      navigate('/pim/viewPhotograph/empNumber/{empNumber}', {\n        empNumber: props.employeeId,\n      });\n    };\n\n    return {\n      imgSrc,\n      employeeName,\n      isCurrentEmp,\n      onClickProfilePic,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.XHRM-card-container {\n  padding: unset;\n}\n\n.XHRM-edit-employee {\n  display: flex;\n  @include oxd-respond-to('xs') {\n    flex-direction: column;\n  }\n  @include oxd-respond-to('md') {\n    flex-direction: row;\n  }\n  &-navigation {\n    width: 100%;\n    padding: 1rem;\n    box-sizing: border-box;\n    @include oxd-respond-to('md') {\n      width: 220px;\n    }\n  }\n  &-content {\n    flex: 1;\n    @include oxd-respond-to('md') {\n      border-left: $oxd-input-control-border--active;\n    }\n  }\n  &-name {\n    text-align: center;\n    padding-left: 1rem;\n    padding-right: 1rem;\n    word-break: break-word;\n    & .--strong {\n      font-weight: 700;\n      font-size: 1.2rem;\n    }\n  }\n  &-image-wrapper {\n    padding-bottom: 1.2rem;\n    @include oxd-respond-to('md') {\n      padding-top: 1.2rem;\n    }\n  }\n  &-image {\n    width: 120px;\n    height: 120px;\n    border-radius: 100%;\n    display: flex;\n    cursor: pointer;\n    overflow: hidden;\n    justify-content: center;\n    box-sizing: border-box;\n    border: 0.5rem solid $oxd-background-pastel-white-color;\n    box-shadow: 1px 1px 18px 11px hsl(238deg 13% 76% / 24%);\n  }\n  &-imagesection {\n    display: flex;\n    align-items: center;\n    @include oxd-respond-to('xs') {\n      flex-direction: row-reverse;\n      justify-content: flex-end;\n    }\n    @include oxd-respond-to('md') {\n      flex-direction: column;\n      justify-content: center;\n    }\n  }\n}\n</style>\n\r\n"],"mappings":";;EAoBOA,KAAK,EAAC;AAA2B;;EAC/BA,KAAK,EAAC;AAAqB;;EACzBA,KAAK,EAAC;AAAoB;;EACxBA,KAAK,EAAC;AAA+B;;EACnCA,KAAK,EAAC;AAAiC;;EACrCA,KAAK,EAAC;AAAyB;;EAM/BA,KAAK,EAAC;AAAkC;;;EAe5CA,KAAK,EAAC;AAA4B;;;;;;uBA1B7CC,mBAAA,CA2CM,OA3CNC,UA2CM,GA1CJC,mBAAA,CAyCM,OAzCNC,UAyCM,GAxCJD,mBAAA,CAuCM,OAvCNE,UAuCM,GAtCJF,mBAAA,CAsBM,OAtBNG,UAsBM,GArBJH,mBAAA,CAmBM,OAnBNI,UAmBM,GAlBJJ,mBAAA,CAKM,OALNK,UAKM,GAJJC,YAAA,CAAiEC,mBAAA;IAAvDC,GAAG,EAAC,IAAI;IAACX,KAAK,EAAC;;sBAAW,MAAkB,C,kCAAfY,MAAA,CAAAC,YAAY,M;;OAClCD,MAAA,CAAAE,YAAY,I,cAA7BC,YAAA,CAEWL,mBAAA;;IAFoBM,IAAI,EAAC;;sBAClC,MAAiC,C,kCAA9BC,IAAA,CAAAC,EAAE,+B;;yCAGTf,mBAAA,CAWM,OAXNgB,UAWM,GAVJhB,mBAAA,CASM;IARJH,KAAK,EAAC,0BAA0B;IAC/BoB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEV,MAAA,CAAAW,iBAAA,IAAAX,MAAA,CAAAW,iBAAA,IAAAD,IAAA,CAAiB;MAEzBnB,mBAAA,CAIE;IAHAqB,GAAG,EAAC,iBAAiB;IACrBxB,KAAK,EAAC,gBAAgB;IACrByB,GAAG,EAAEb,MAAA,CAAAc;iCAKdjB,YAAA,CAAgDkB,0BAAA;IAA9BC,IAAI,EAAEC,MAAA,CAAAD;EAAI,sB,GAE9BzB,mBAAA,CAcM,OAdN2B,UAcM,GAbJC,WAAA,CAAad,IAAA,CAAAe,MAAA,iBAAAC,SAAA,SAELJ,MAAA,CAAAK,MAAM,kB,cADdnB,YAAA,CAIyBoB,gCAAA;;IAFtB,aAAW,EAAEN,MAAA,CAAAO,UAAU;IACvBF,MAAM,EAAEL,MAAA,CAAAK;2EAGHL,MAAA,CAAAK,MAAM,kB,cADdnB,YAAA,CAMuBsB,8BAAA;;IAJpB,aAAW,EAAER,MAAA,CAAAO,UAAU;IACvB,oBAAkB,EAAEP,MAAA,CAAAS,gBAAgB;IACpC,eAAa,EAAET,MAAA,CAAAU,WAAW;IAC1BL,MAAM,EAAEL,MAAA,CAAAK","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}