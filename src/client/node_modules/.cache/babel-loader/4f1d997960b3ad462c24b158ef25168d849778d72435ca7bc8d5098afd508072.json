\n            .getAll({\n              nameOrId: searchParam.trim(),\n              ...this.params,\n            })\n            .then(({data}) => {\n              resolve(\n                data.data\n                  .filter(\n                    (employee) =>\n                      employee.empNumber !== this.authEmployeeNumber,\n                  )\n                  .map((employee) => {\n                    return {\n                      id: employee.empNumber,\n                      label: `${employee.firstName} ${employee.middleName} ${employee.lastName}`,\n                      _employee: employee,\n                      isPastEmployee: employee.terminationId ? true : false,\n                    };\n                  }),\n              );\n            });\n        } else {\n          resolve([]);\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.past-employee-tag {\n  margin-left: auto;\n}\n</style>\n"],"mappings":";;;EAiCsCA,KAAK,EAAC;;;;uBAb1CC,YAAA,CAiBkBC,0BAAA;IAhBhBC,IAAI,EAAC,cAAc;IAClBC,KAAK,EAAEC,IAAA,CAAAC,EAAE;IACTC,KAAK,EAAE,KAAK;IACZ,gBAAc,EAAEC,QAAA,CAAAC;;IAENC,aAAa,EAAAC,QAAA,CAFb,CAGE;MADcC;IAAI,OACbA,IAAI,CAACC,cAAc,I,cAAnCC,mBAAA,CAEWC,SAAA;MAAAC,GAAA;IAAA,I,kCADNX,IAAA,CAAAC,EAAE,+B;IAGEW,MAAM,EAAAN,QAAA,CACf,CAA6B;MADXC;IAAI,OACtBM,mBAAA,CAA6B,cAAAC,gBAAA,CAApBP,IAAI,CAACR,KAAK,OACRQ,IAAI,CAACC,cAAc,I,cAA9BC,mBAAA,CAEM,OAFNM,UAEM,EAAAD,gBAAA,CADDd,IAAA,CAAAC,EAAE,kC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}