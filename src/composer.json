{
    "name": "xhrm/xhrm",
    "description": "XHRM open source",
    "keywords": [
        "orangehrm"
    ],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4|^8.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-simplexml": "*",
        "ext-gd": "*",
        "ext-ldap": "*",
        "ext-openssl": "*",
        "symfony/http-foundation": "~5.4.48",
        "symfony/routing": "~5.4.48",
        "symfony/http-kernel": "~5.4.48",
        "symfony/event-dispatcher": "~5.4.45",
        "respect/validation": "~2.2.4",
        "doctrine/orm": "~2.20.7",
        "doctrine/annotations": "~1.14.4",
        "symfony/yaml": "~5.4.45",
        "twig/twig": "~3.11.3",
        "symfony/error-handler": "~5.4.46",
        "symfony/dependency-injection": "~5.4.48",
        "symfony/cache": "~5.4.46",
        "symfony/mailer": "~5.4.45",
        "symfony/security-csrf": "~5.4.45",
        "guzzlehttp/guzzle": "~7.8.1",
        "symfony/translation": "~5.4.45",
        "symfony/polyfill-intl-messageformatter": "~1.29.0",
        "symfony/process": "~5.4.47",
        "scssphp/scssphp": "~1.12.1",
        "phpseclib/phpseclib": "~3.0.37",
        "symfony/ldap": "~5.4.45",
        "symfony/console": "~5.4.47",
        "crunzphp/crunz": "~3.4.1",
        "bjeavons/zxcvbn-php": "~1.3.1",
        "league/oauth2-server": "~8.4.2",
        "psr/http-message": "~1.1.0",
        "symfony/psr-http-message-bridge": "~2.3.1",
        "nyholm/psr7": "~1.8.1",
        "jumbojett/openid-connect-php": "~1.0.0",
        "enshrined/svg-sanitize": "~0.22.0",
        "monolog/monolog": "~2.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "zircote/swagger-php": "~4.8.4"
    },
    "autoload": {
        "psr-4": {
            "XHRM\\ORM\\": "./lib/orm",
            "XHRM\\Framework\\": "./lib/framework",
            "XHRM\\Config\\": "./lib/config",
            "XHRM\\Admin\\": "./plugins/orangehrmAdminPlugin",
            "XHRM\\Attendance\\": "./plugins/orangehrmAttendancePlugin",
            "XHRM\\Authentication\\": "./plugins/orangehrmAuthenticationPlugin",
            "XHRM\\Buzz\\": "./plugins/orangehrmBuzzPlugin",
            "XHRM\\OAuth\\": "./plugins/orangehrmCoreOAuthPlugin",
            "XHRM\\Core\\": "./plugins/orangehrmCorePlugin",
            "XHRM\\CorporateDirectory\\": "./plugins/orangehrmCorporateDirectoryPlugin",
            "XHRM\\Dashboard\\": "./plugins/orangehrmDashboardPlugin",
            "XHRM\\Help\\": "./plugins/orangehrmHelpPlugin",
            "XHRM\\Leave\\": "./plugins/orangehrmLeavePlugin",
            "XHRM\\Maintenance\\": "./plugins/orangehrmMaintenancePlugin",
            "XHRM\\OpenidAuthentication\\": "./plugins/orangehrmOpenidAuthenticationPlugin",
            "XHRM\\Performance\\": "./plugins/orangehrmPerformancePlugin",
            "XHRM\\Pim\\": "./plugins/orangehrmPimPlugin",
            "XHRM\\Recruitment\\": "./plugins/orangehrmRecruitmentPlugin",
            "XHRM\\Time\\": "./plugins/orangehrmTimePlugin",
            "XHRM\\I18N\\": "./plugins/orangehrmI18NPlugin",
            "XHRM\\CorporateBranding\\": "./plugins/orangehrmCorporateBrandingPlugin",
            "XHRM\\LDAP\\": "./plugins/orangehrmLDAPAuthenticationPlugin",
            "XHRM\\SystemCheck\\": "./plugins/orangehrmSystemCheckPlugin",
            "XHRM\\Mobile\\": "./plugins/orangehrmMobilePlugin",
            "XHRM\\Claim\\": "./plugins/orangehrmClaimPlugin",
            "XHRM\\Installer\\": "../installer",
            "XHRM\\Entity\\": [
                "./plugins/orangehrmAdminPlugin/entity",
                "./plugins/orangehrmPerformancePlugin/entity",
                "./plugins/orangehrmPimPlugin/entity",
                "./plugins/orangehrmLeavePlugin/entity",
                "./plugins/orangehrmCorePlugin/entity",
                "./plugins/orangehrmCoreOAuthPlugin/entity",
                "./plugins/orangehrmAuthenticationPlugin/entity",
                "./plugins/orangehrmTimePlugin/entity",
                "./plugins/orangehrmAttendancePlugin/entity",
                "./plugins/orangehrmI18NPlugin/entity",
                "./plugins/orangehrmRecruitmentPlugin/entity",
                "./plugins/orangehrmCorporateBrandingPlugin/entity",
                "./plugins/orangehrmLDAPAuthenticationPlugin/entity",
                "./plugins/orangehrmBuzzPlugin/entity",
                "./plugins/orangehrmClaimPlugin/entity",
                "./plugins/orangehrmOpenidAuthenticationPlugin/entity"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XHRM\\Tests\\": "./test/phpunit",
            "XHRM\\Tests\\Admin\\": "./plugins/orangehrmAdminPlugin/test",
            "XHRM\\Tests\\Authentication\\": "./plugins/orangehrmAuthenticationPlugin/test",
            "XHRM\\Tests\\Core\\": "./plugins/orangehrmCorePlugin/test",
            "XHRM\\Tests\\Leave\\": "./plugins/orangehrmLeavePlugin/test",
            "XHRM\\Tests\\Pim\\": "./plugins/orangehrmPimPlugin/test",
            "XHRM\\Tests\\OAuth\\": "./plugins/orangehrmCoreOAuthPlugin/test",
            "XHRM\\Tests\\Time\\": "./plugins/orangehrmTimePlugin/test",
            "XHRM\\Tests\\Attendance\\": "./plugins/orangehrmAttendancePlugin/test",
            "XHRM\\Tests\\Maintenance\\": "./plugins/orangehrmMaintenancePlugin/test",
            "XHRM\\Tests\\I18N\\": "./plugins/orangehrmI18NPlugin/test",
            "XHRM\\Tests\\Recruitment\\": "./plugins/orangehrmRecruitmentPlugin/test",
            "XHRM\\Tests\\Performance\\": "./plugins/orangehrmPerformancePlugin/test",
            "XHRM\\Tests\\CorporateDirectory\\": "./plugins/orangehrmCorporateDirectoryPlugin/test",
            "XHRM\\Tests\\CorporateBranding\\": "./plugins/orangehrmCorporateBrandingPlugin/test",
            "XHRM\\Tests\\Dashboard\\": "./plugins/orangehrmDashboardPlugin/test",
            "XHRM\\Tests\\LDAP\\": "./plugins/orangehrmLDAPAuthenticationPlugin/test",
            "XHRM\\Tests\\Buzz\\": "./plugins/orangehrmBuzzPlugin/test",
            "XHRM\\Tests\\Help\\": "./plugins/orangehrmHelpPlugin/test",
            "XHRM\\Tests\\Mobile\\": "./plugins/orangehrmMobilePlugin/test",
            "XHRM\\Tests\\Claim\\": "./plugins/orangehrmClaimPlugin/test",
            "XHRM\\Tests\\OpenidAuthentication\\": "./plugins/orangehrmOpenidAuthenticationPlugin/test"
        }
    },
    "scripts": {
        "test": "cd ../ && phpunit",
        "post-autoload-dump": [
            "php ../bin/console orm:generate-proxies",
            "php ../bin/console cache:clear"
        ]
    }
}